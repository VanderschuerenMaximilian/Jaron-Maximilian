{
  "name": "jaron-maximilian",
  "version": "1.0.0",
  "description": "Project Advanced Full Stack Developement",
  "scripts": {
    "dev": "npx lerna run --scope={pwa,api} dev",
    "export:translations": "npx lerna run --scope=i18n-export export",
    "dev:api": "npx lerna run --scope=api dev",
    "dev:pwa": "npx lerna run --scope=pwa dev",
    "seed:all": "npx lerna run --scope=api seed:all",
    "seed:reset": "npx lerna run --scope=api seed:reset",
    "test:all": "npx lerna run --scope=api test:all"
  },
  "keywords": [],
  "author": "Jaron Maximilian",
  "bugs": {
    "url": "https://github.com/VanderschuerenMaximilian/Jaron-Maximilian/issues"
  },
  "homepage": "https://github.com/VanderschuerenMaximilian/Jaron-Maximilian",
  "workspaces": [
    "packages/*"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
  },
  "dependencies": {},
  "devDependencies": {
    "lerna": "^7.4.1"
  }
}
